---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Ack &amp; Nak - Everything but no mayo</title>
 <link href="http://ackandnak.com/feed.xml" rel="self"/>
 <link href="http://ackandnak.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://ackandnak.com/</id>
 <author>
   <name>Justin Knowlden</name>
   <email>gus@gusg.us</email>
 </author>
 <author>
   <name>Carl Brahms</name>
   <email>carl.brahms@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   <link href="http://ackandnak.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://ackandnak.com{{ post.id }}</id>
   <content type="html">
     <img src="/images/comics/{{ post.comic }}" width="{{ post.width }}" height="{{ post.height }}" alt="{{ post.alt }}" title="{{ post.alt }}" />
     <div class="caption">{{ post.content | xml_escape }}</div>
   </content>
 </entry>
 {% endfor %}
 
</feed>